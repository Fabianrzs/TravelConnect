{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Encryption": {
    "Key": "{Key}"
  },
  "JwtSettings": {
    "Issuer": "TravelConnect",
    "Audience": "TravelConnect",
    "Key": "{Key}"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server={server},1433; User ID={user}; Password={password}; Initial Catalog={database}; MultipleActiveResultSets=true; TrustServerCertificate=true;"
  },
  "EmailSettings": {
    "SmtpHost": "smtp.gmail.com",
    "SmtpUser": "{SmtpUser}",
    "SmtpPassword": "{SmtpPassword}",
    "SmtpPort": "587"
  },
  "AllowedHosts": "*"
}
